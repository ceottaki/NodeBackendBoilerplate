{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Main",
            "type": "node",
            "request": "launch",
            "args": [
                "./src/main.ts"
            ],
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector"
        }
    ],
}
